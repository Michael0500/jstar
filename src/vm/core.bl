def assert(cond, msg="assertion failed")
	if !cond then 
		raise AssertException(msg) 
	end
end

def print(o="", file=null)
	write(o, file)
	__printstr("\n", file)
end

def write(o, file=null)
	var s = str(o)
	assert(s is String, "__string__() didn't return a string, got " + s.__class__().getName())
	__printstr(s, file)
end

def str(e)
	return e.__string__()
end

class range
	def new(from, to=null, step=1)
		if to == null then
			to = from
			from = 0
		end

		if !isInt(from) or !isInt(to) then
			raise InvalidArgException("Argument `from` and `to` must be integers")
		end

		this.from = from
		this.to = to
		this.count = from
		this.step = step
	end

	def hasNext()
		return this.count < this.to
	end

	def next()
		var i = this.count
		this.count += this.step
		return i
	end

	def __iterator__()
		return this
	end

	def __string__()
		var s = "["
		for var i = this.from; i < this.to; i += 1 do
			s += i.__string__()
			if i != this.to - 1 then
				s += ", "
			end
		end
		return s + "]"
	end
end

