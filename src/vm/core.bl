class String
	def new()
		__noinstance(this)
	end

	native __eq__(o)
	native substr(from, to)
	native length()

	def join(iterable)
		var lst = iterable
		if !(lst is List) then
			lst = []
			for var e in iterable do
				lst.add(e)
			end
		end
		return this.__join(lst)
	end

	native __join(lst)

	def __string__()
		return this
	end

	def __iterator__()
		return __StringIterator(this)
	end

	native __hash__()
end

class __StringIterator
	def new(str)
		this.str = str
		this.pos = 0
	end

	def hasNext()
		return this.pos < this.str.length()
	end

	def next()
		var c = this.str[this.pos]
		this.pos += 1
		return c
	end
end

class List
	def new()
		__noinstance(this)
	end

	native add(e)
	native insert(i, e)
	native removeAt(i)
	native size()
	native clear()

	def addAll(iterable)
		var changed = false
		for var e in iterable do
			this.add(e)
			changed = true
		end
		return changed
	end

	def insertAll(iterable, i=0)
		if i < 0 or i >= this.size() then
			raise IndexOutOfBoundException(str(i))
		end

		var changed = false
		for var e in iterable do
			this.insert(i, e)
			changed = true
		end
		return changed
	end

	def remove(e)
		var i = this.indexOf(e)
		if i >= 0 then
			this.remove(i)
			return true
		end
		return false
	end

	def removeAll(iterable)
		var changed = false
		for var e in iterable do
			var r = this.remove(e)
			changed = changed or r
		end
		return changed
	end

	def contains(e)
		return this.indexOf(e) != -1
	end

	def indexOf(e)
		for var i = 0; i < this.size(); i += 1 do
			if this[i] == e then
				return i
			end
		end
		return -1
	end

	def indexOfLast(e)
		for var i = this.size() - 1; i >= 0; i -= 1 do
			if this[i] == e then
				return i
			end
		end
		return -1
	end

	def swap(a, b)
		var temp = this[a]
		this[a] = this[b]
		this[b] = temp
	end

	def subList(from, to)
		if to < from then
			raise InvalidArgException("to arg should be >= from arg")
		end

		var sub = []
		for var i = from; i < to; i += 1 do
			sub.add(this[i])
		end
		return sub
	end

	def empty()
		return this.size() == 0
	end

	def sort()
		import rand
		this.__quickSort(0, this.size() - 1)
	end

	def __quickSort(from, to)
		if from < to then
			var p = this.__partition(from, to)

			this.__quickSort(from, p - 1)
			this.__quickSort(p + 1, to)
		end
	end

	def __partition(from, to)
		var p = rand.randint(from, to + 1)
		this.swap(from, p)

		var piv = this[from]
		var l = from + 1
		var r = to

		while l <= r do
			while this[r] > piv do
				r -= 1
			end
			while l <= r and this[l] <= piv do
				l += 1
			end

			if l < r then
				this.swap(l, r)
				l += 1
				r -= 1
			end
		end

		this.swap(from, r)
		return r
	end

	def __iterator__()
		return __ListIterator(this)
	end

	def __string__()
		return "[" + this.join(", ") + "]"
	end

	def __eq__(lst)
		if this.size() != lst.size() then
			return false
		end
		for var i in range(this.size()) do
			if this[i] != lst[i] then
				return false
			end
		end
		return true
	end
end

class __ListIterator
	def new(list)
		this.n = 0
		this.list = list
	end

	def hasNext()
		return this.n < this.list.size()
	end

	def next()
		var next = this.list[this.n]
		this.n += 1
		return next
	end
end

class __ListReverseIterator
	def new(list)
		this.n = list.size() - 1
		this.list = list
	end

	def hasNext()
		return this.n >= 0
	end

	def next()
		var prev = this.list[this.n]
		this.n -= 1
		return prev
	end

	def __iterator__()
		return this
	end
end

def reversed(lst)
	return __ListReverseIterator(lst)
end

def __noinstance(inst)
	raise Exception("Class " + inst.__class__().getName() + " cannot be directly instantiated")
end

