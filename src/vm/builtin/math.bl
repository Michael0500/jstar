native random()
native seed(s)

fun randint(a, b=null)
    if !isInt(a) or (b and !isInt(b)) then
        raise InvalidArgException("a and b must be integers")
    end
    if b == null then
        a, b = 0, a
    end
    if a >= b then 
        raise InvalidArgException("a must be < b") 
    end
    return int(a + random() * (b - a + 1))
end

